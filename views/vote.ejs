<body class="vote-body">

    <!-- Heading -->
    <div class="container-fluid trans ">
        <h3 class="vote-head"><i class="fab fa-dyalog"></i><i class="fab fa-vuejs vote-icon"></i> DeployVotes</h3>
        <div class=" vote-headbox text-center ">
            <div class="vote-box gra-img">
                <h1 class="welcome-msg">Welcome Sowbhakian, Have a great day!</h1>
                <div class="row">

                    <div class="col-lg-6 col-md-6 col-sm-12 ">
                        <h2 class="sts-msg voteMsgRt ">Ends on - 12 Aug 2021</h2>
                        <h2 class="sts-msg voteMsgRt"> 09:00 AM-06:00 PM</h2>
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-12 ">
                        <h2 class="sts-msg voteMsgLt">Enrollment Status : Done</h2>
                        <h2 class="sts-msg voteMsgLt">Voting Status : Not Yet</h2>
                    </div>
                </div>
                <div class="vote-return">
                    <a href="/">
                        <button type="button" class="btn btn-lg btn-outline-light">Return Home <i class="fas fa-angle-double-right"></i></button>
                    </a>
                </div>
            </div>
        </div>
    </div>

    <div class="container">

        <h1 class="voting-title"> Vote Your Candidate</h1>

        <div class="border border-dark voting-iconDiv">
            <div class="row text-center">
                <div class="col-lg-4 col-md-4 symbol-inp-div">
                    <i class="fas fa-lightbulb fa-10x vote-IC"></i>
                    <h2 class="vote-partyName">Party Name</h2>
                    <button type="button" class="btn btn-primary btn-lg vote-btn">VOTE</button>
                </div>
                <div class="col-lg-4 col-md-4 symbol-inp-div">
                    <i class="fas fa-gavel fa-10x vote-IC"></i>
                    <h2 class="vote-partyName">Party Name</h2>
                    <button type="button" class="btn btn-primary btn-lg vote-btn">VOTE</button>
                </div>
                <div class="col-lg-4 col-md-4 symbol-inp-div">
                    <i class="fas fa-broom fa-10x vote-IC"></i>
                    <h2 class="vote-partyName">Party Name</h2>
                    <button type="button" class="btn btn-primary btn-lg vote-btn">VOTE</button>
                </div>
                <div class="col-lg-4 col-md-4 symbol-inp-div">
                    <i class="fab fa-angellist fa-10x vote-IC"></i>
                    <h2 class="vote-partyName">Party Name</h2>
                    <button type="button" class="btn btn-primary btn-lg vote-btn">VOTE</button>
                </div>
                <div class="col-lg-4 col-md-4 symbol-inp-div">
                    <i class="fas fa-universal-access fa-10x vote-IC"></i>
                    <h2 class="vote-partyName">Party Name</h2>
                    <button type="button" class="btn btn-primary btn-lg vote-btn">VOTE</button>
                </div>
                <div class="col-lg-4 col-md-4 symbol-inp-div">
                    <i class="far fa-bell fa-10x vote-IC"></i>
                    <h2 class="vote-partyName">Party Name</h2>
                    <button type="button" class="btn btn-primary btn-lg vote-btn">VOTE</button>
                </div>
            </div>
        </div>
    </div>


    <!-- footer-section -->
    <section id="footer-section ">
        <footer>
            <p class="cpy-footer "> <i class="far fa-copyright "></i> 2021 Sowbhakian
            </p>
        </footer>
    </section>


